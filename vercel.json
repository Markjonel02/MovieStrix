{
  "builds": [
    { "src": ".Server/Server.mjs", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "Server" }
  ],
  
    "build": [
      {"postbuild": "node .Server/Server.mjs"}
    ],

  
   "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ] 
}

